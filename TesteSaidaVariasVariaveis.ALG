Algoritmo "teste_saida"

Var
   idade : inteiro
   salario : real
   nome : caractere
   
Inicio
   idade <- 25
   salario <- 4560.9
   nome <- "Matt Lindo Rocha"
   
   escreval("O funcionario ", nome, " ganha ", salario:8:2, " e tem ", idade, " anos.")
   escreval
   escreval("Bom dia!")
   escreval
   escreval("Boa Noite!")

Fimalgoritmo